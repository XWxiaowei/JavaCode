<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.jay.ssh.entity.Employee" table="sh_employee" schema="ssh_employee">
        <id name="eid">
            <column name="eid" sql-type="int(11)"/>
            <generator class="native" />
        </id>
        <property name="did">
            <column name="did" sql-type="int(11)"/>
        </property>
        <property name="ename">
            <column name="ename" sql-type="varchar(150)" length="150" not-null="true"/>
        </property>
        <property name="sex">
            <column name="sex" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="birthday">
            <column name="birthday" sql-type="datetime" not-null="true"/>
        </property>
        <property name="joinDate">
            <column name="joinDate" sql-type="datetime" not-null="true"/>
        </property>
        <property name="eno">
            <column name="eno" sql-type="varchar(150)" length="150" not-null="true"/>
        </property>
        <property name="username">
            <column name="username" sql-type="varchar(150)" length="150" not-null="true"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(150)" length="150" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>